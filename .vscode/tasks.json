{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "${workspaceRoot}/build/moex-cli",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "echoCommand": true,
    "args": [
        "--file",
        "/Applications/SizeOptDemo"
    ],
    "tasks": [
        {
            "taskName": "arch list",
            "args": [
                "--arch_list"
            ]
        },
        {
            "taskName": "section list",
            "args": [
                "--section_list"
            ]
        },
        {
            "taskName": "symbol list",
            "args": [
                "--symbol_list"
            ]
        },
        {
            "taskName": "string table",
            "args": [
                "--string_table"
            ]
        },
        {
            "taskName": "help",
            "args": [
                "--help"
            ]
        }
    ]
}